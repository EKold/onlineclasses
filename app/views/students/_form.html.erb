<%= form_for([@online, @online.students.build]) do |f| %>
    <p>
        <%= f.label :name %><br>
        <%= f.text_field(:name, {:class => 'form-control'} ) %>
    </p>
    
    <p>
        <%= f.label :email %><br>
        <%= f.text_area( :email, {:class => 'form-control'} )%>
    </p>
    
    <p>
        <%= f.label :here %><br>
        <%= f.check_box( :here, {:class => 'form-control'} ) %>
        
    </p>

    <p>
        <%= f.submit({:class => 'btn btn-defualt'}) %>
    </p>
<% end %>